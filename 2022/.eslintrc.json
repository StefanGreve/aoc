{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "extends": [
        "plugin:@typescript-eslint/recommended",
        "plugin:prettier/recommended",
        "prettier"
    ],
    "rules": {
        "no-shadow": "warn",
        "no-unused-vars": "off",
        "prefer-const": "warn",
        "prefer-rest-params": "warn",
        "prefer-spread": "warn"
    }
}
